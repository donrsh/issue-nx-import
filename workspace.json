{
  "version": 2,
  "projects": {
    "hook-use-counter": {
      "root": "libs/hook/use-counter",
      "sourceRoot": "libs/hook/use-counter/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/hook/use-counter/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hook/use-counter"],
          "options": {
            "jestConfig": "libs/hook/use-counter/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "next-todo": {
      "root": "apps/next-todo",
      "sourceRoot": "apps/next-todo",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/next-todo",
            "outputPath": "dist/apps/next-todo"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "next-todo:build",
            "dev": true,
            "port": 4200
          },
          "configurations": {
            "production": {
              "buildTarget": "next-todo:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "next-todo:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/next-todo"],
          "options": {
            "jestConfig": "apps/next-todo/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/next-todo/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "next-todo-e2e": {
      "root": "apps/next-todo-e2e",
      "sourceRoot": "apps/next-todo-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/next-todo-e2e/cypress.json",
            "tsConfig": "apps/next-todo-e2e/tsconfig.e2e.json",
            "devServerTarget": "next-todo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "next-todo:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/next-todo-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shop": {
      "root": "apps/shop",
      "sourceRoot": "apps/shop/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/shop",
            "index": "apps/shop/src/index.html",
            "main": "apps/shop/src/main.tsx",
            "polyfills": "apps/shop/src/polyfills.ts",
            "tsConfig": "apps/shop/tsconfig.app.json",
            "assets": ["apps/shop/src/favicon.ico", "apps/shop/src/assets"],
            "styles": ["apps/shop/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/shop/src/environments/environment.ts",
                  "with": "apps/shop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "shop:build",
            "hmr": true,
            "port": 4300
          },
          "configurations": {
            "production": {
              "buildTarget": "shop:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shop/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/shop"],
          "options": {
            "jestConfig": "apps/shop/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/shop/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shop",
            "config": {
              "configFolder": "apps/shop/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shop-e2e": {
      "root": "apps/shop-e2e",
      "sourceRoot": "apps/shop-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/shop-e2e/cypress.json",
            "tsConfig": "apps/shop-e2e/tsconfig.e2e.json",
            "devServerTarget": "shop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "shop:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/shop-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "pascalCaseFiles": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "shop"
}
